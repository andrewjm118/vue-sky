{
  "name": "vue-sky",
  "version": "1.0.0",
  "description": "vue2.0 Bulma ui 组件",
  "author": "Andrew Jm <63901040@qq.com>",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "unpkg": "dist/vue-sky.min.js",
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "ui",
    "vue",
    "vuejs",
    "vue-sky",
    "components"
  ],
  "jest": {
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  },
  "scripts": {
    "dev": "webpack-dev-server --inline  --config build/webpack.config.dev.js",
    "prod": "webpack --config build/webpack.config.prod.js",
    "lint": "eslint --ext .js,.vue src",
    "lint:fix": "eslint --ext .js,.vue src --fix",
    "new": "plop",
    "test": "jest",
    "build:js": "rollup -c && rollup -c --environment MINIFY",
    "build:scss": "node-sass --output-style expanded src/scss/vue-sky-build.scss | node ./build/banner.js > dist/vue-sky.css && cleancss -o dist/vue-sky.min.css dist/vue-sky.css",
    "build:lib": "npm run build:js && npm run build:scss",
    "build:docs": "node build/build.js",
    "build": "npm run build:lib && npm run build:docs"
  },
  "dependencies": {
    "@mdi/font": "^4.3.95",
    "async-validator": "1.8.1",
    "bulma": "^0.7.5",
    "gemini-scrollbar": "^1.5.3",
    "jump.js": "^1.0.2",
    "liquor-tree": "^0.2.70",
    "portfinder": "^1.0.28",
    "vee-validate": "^3.2.1",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vue-sidebar-menu": "^4.5.3"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@fortawesome/fontawesome-free": "^5.11.1",
    "@testing-library/vue": "^3.0.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "clean-css-cli": "^4.3.0",
    "clipboard": "^2.0.4",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "eslint": "^6.3.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^5.2.3",
    "extract-css-chunks-webpack-plugin": "^4.9.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^4.2.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "jest-serializer-vue": "^2.0.2",
    "json-loader": "^0.5.7",
    "loader-utils": "^1.2.3",
    "lodash.template": "^4.5.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^7.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "plop": "^2.4.0",
    "postcss-loader": "^5.0.0",
    "raw-loader": "^3.1.0",
    "resolve-url-loader": "^3.1.2",
    "rollup": "^1.23.1",
    "rollup-copy-plugin": "^0.1.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.1.2",
    "rollup-plugin-vue": "^5.0.1",
    "sass-loader": "^8.0.0",
    "script-ext-html-webpack-plugin": "^2.1.5",
    "shelljs": "^0.8.4",
    "style-loader": "^1.0.0",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "transliteration": "^2.1.7",
    "uglify-es": "^3.3.9",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^2.1.0",
    "vue-jest": "^3.0.4",
    "vue-loader": "^15.7.1",
    "vue-md-loader": "^1.1.0",
    "vue-server-renderer": "^2.6.10",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.2",
    "yaml-loader": "^0.5.0"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  }
}
